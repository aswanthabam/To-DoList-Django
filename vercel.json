{
  "version": 2,
  "builds": [
    {
      "src": "todolist/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "buildscript.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "todolist/wsgi.py"
    }
  ],
  "outputDirectory": "static"
}